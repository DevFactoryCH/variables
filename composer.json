{
  "name": "devfactory/variables",
  "description": "Laravel package to allow site owners to override a specific config files' variables",
  "license": "MIT",
  "authors": [
    {
      "name": "DevFactory",
      "email": "developer@devfactory.ch"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "illuminate/support": "5.*"
  },
  "require-dev": {
    "mockery/mockery": "dev-master@dev",
    "codeclimate/php-test-reporter": "dev-master"
  },
  "autoload": {
    "classmap": [
      "src/migrations"
    ],
    "psr-4": {
      "Devfactory\\Variables\\": "src/"
    }
  },
  "minimum-stability": "stable"
}
